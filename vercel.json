{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "DATABASE_URL": "sqlite:///./todo_chatbot.db",
    "JWT_SECRET_KEY": "$JWT_SECRET_KEY",
    "OPENAI_API_KEY": "$OPENAI_API_KEY",
    "BETTER_AUTH_SECRET": "$BETTER_AUTH_SECRET",
    "CSRF_SECRET_KEY": "$CSRF_SECRET_KEY"
  }
}