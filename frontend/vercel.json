{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": { "buildCommand": "npm run build" }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/" }],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "https://ubaid-ai-bot.hf.space",
    "NEXT_PUBLIC_ENABLE_ANALYTICS": "false",
    "NEXT_PUBLIC_MAINTENANCE_MODE": "false"
  }
}
